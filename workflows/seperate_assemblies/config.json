{
    "global_tmp_dir": "$SNIC_TMP",
    "bowtie2_rules": {
        "load_env": "module load bioinfo-tools bowtie2/2.2.6 samtools/1.1",
        "platform": "Illumina",
        "mapping_params": {
            "default": "",
            "local": "--local",
            "local_very_sensitive": "--very-sensitive-local"
        }
    },
    "samtools_rules": {
        "load_env": "module load bioinfo-tools samtools/1.3 BEDTools/2.21.0 picard/1.118",
        "picard_jars": "/sw/apps/bioinfo/picard/1.118/milou",
        "threads_for_sorting": "16"
    },
    "mapping_report_rules": {},
    "kallisto_rules": {
        "load_env": "module load bioinfo-tools",
        "inputtable_script":  "toolbox/scripts/kallisto_concoct/input_table.py"
    },
    "gattaca_rules": {
        "load_env": "module load gcc/4.9.4 && export PATH=/proj/b2017017/bin:$PATH"
    },
    "khmer_rules": {
        "load_env": "module load bioinfo-tools khmer/2.1.1 && export PATH=/proj/b2017017/bin:$PATH",
        "inputtable_script":  "toolbox/scripts/khmer_concoct/input_table.py"
    },
    "metabat_rules": {
        "load_default_env": "source activate binning_checkm && module load bioinfo-tools hmmer",
        "binning_params": {
            "default": {
                "load_env": "module load bioinfo-tools MetaBat",
                "commandline_params": "--minContig 1500"
            },
            "maxEdge500": {
                "load_env": "module load bioinfo-tools MetaBat",
                "commandline_params": "--minContig 1500 --maxEdge 500"
            },
        },
        "quant_commandline_params": {
            "default": "--cvExt",
            "kallisto": "--cvExt",
            "default_jgi": ""
        },
        "quant_params": {
            "default": {
                "input_table": "concoct_inputtable.sorted.tsv"
            },
            "default_jgi": {
                "input_table": "depth_10K.tsv"
            },
            "kallisto": {
                "input_table": "concoct_inputtable.sorted.tsv"
            }
        }
    },
    "binning_rules": {
        "mapper": "bowtie2",
        "mapping_params": "local",
        "load_default_env": "source activate concoct_0.4.1 && module load bioinfo-tools hmmer/3.1b2 prodigal/2.6.3 samtools/1.5",
        "load_checkm_env": "source activate binning_checkm && module load bioinfo-tools hmmer",
        "scripts_dir": "CONCOCT/scripts",
        "approve_bins_script": "toolbox/scripts/concoct/approve_bins_from_checkm.py",
        "min_completeness": "75",
        "max_contamination": "5",
        "dnadiff_tmp_dir": "$SNIC_TMP",
        "merge_clustering_script": "toolbox/scripts/concoct/merge_cutup_clustering.py",
        "extract_bins_script": "toolbox/scripts/concoct/extract_fasta_bins.py",
        "correctly_placed_16S_script": "toolbox/scripts/concoct/correctly_placed_16S.py"
    },
